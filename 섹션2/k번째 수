
T =int(input())  #case를 읽음
for t in range(T): #case만큼 돎!
    n, s, e, k= map(int, input().split()) # map은 리스트의 요소를 지정된 함수 #몇번 도는지 변수
    a = list(map(int, input().split()))  #아래 리스트들!
    print(a)
    a=a[s-1:e] #s번쨰 index에서 e번 index까지
    a.sort() #오름차순 정렬
    print("#%d %d" %(t, a[k-1]))  #K번재 ㅑndex 출력, t번째 case에선, 뭐뭐

