import sys
sys.stdin=open("input.txt", "rt")
N= int(input())
k=list(map(int,input().split()))
print(k)  #중요!!!
ave=round(sum(k)/N)#round는 소수 첫째자리,sum은 list의 모든값을 합해줌
min =2151341
for idx, x in enumerate(k): #idx는 인덱스 넘버, enumerate는 k의 index 값을 반환
    tmp=abs(x-ave)#abs는 절대값
    if tmp <min:
        min =tmp #min에 tmp 