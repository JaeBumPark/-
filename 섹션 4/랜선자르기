import sys
sys.stdin=open("input4.txt", "rt")
def Count(len): #
     cnt=0
     for x in L:
        cnt+=(x//len)
     return cnt  
N, M=map(int, input().split())
L=[]
res=0
largest=0
lt=1
rt=largest   
for i in range(N):
    tmp=int(input())
    L.append(tmp) #list에 추가
    largest=max(largest, tmp)

while lt<=rt:
    mid=(lt+rt)//2
    if Count(mid)>=M:
        res=mid
        lt=mid+1
    else:
        rt=mid-1
print(res)       
